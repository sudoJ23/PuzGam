<?xml version='1.0' encoding='utf-8'?>
<widget id="org.j23.puzgam" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Exit">
        <param name="android-package" value="im.ckk.cordova.exit.Exit" />
    </feature>
    <feature name="ExitApp">
        <param name="android-package" value="kr.co.joycorp.cordova.exitapp" />
    </feature>
    <name>puzgam</name>
    <description>
        Puzzle game
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        XYZ team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon src="www/img/icon/icon.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
